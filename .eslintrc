{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "vue/require-default-prop": "off",
    "vue/multi-word-component-names": "off"
  },
  "globals": {
    "computed": true,
    "defineEmits": true,
    "defineProps": true,
    "ref": true,
    "useAttrs": true,
    "onMounted": true,
    "onUnmounted": true,
    "watch": true,
    "useForm": true,
    "usePage": true
  }
}